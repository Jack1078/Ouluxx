{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "jquery": true,
    "node": true
  },
  "extends": ["google", "react-app"],
  "globals": {},
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-fragments": 0,
    "react-hooks/rules-of-hooks": 1
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
